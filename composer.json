{
    "name": "tom-shelby/hephaestus-framework",
    "description": "The Laravel Zero Framework.",
    "keywords": [
        "framework",
        "laravel",
        "discord",
        "laravel zero"
    ],
    "homepage": "https://laravel-zero.com",
    "authors": [
        {
            "name": "Tom Shelby",
            "homepage": "https://github.com/tom-shelby"
        }
    ],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/log": "^10.0",
        "laravel-zero/framework": "^10.2",
        "nunomaduro/termwind": "^1.15.1",
        "team-reflex/discord-php": "^7.3"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.22"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "hephaestus-framework"
    ]
}
